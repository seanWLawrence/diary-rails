{
  "extends": [
    "tslint:latest",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "error",
  "rules": {
    "interface-name": false,
    "max-line-length": {
      "options": [80]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    },
    "prefer-const": false,
    "prettier": true
  },
  "jsRules": {
    "max-line-length": {
      "options": [80]
    },
    "prefer-const": false,
    "prettier": true
  }
}
